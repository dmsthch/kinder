	(
	#{license_kindergarten}
	,#{teacher_level}
	,#{teacher_name}
	,#{teacher_id}
	,#{teacher_pw}
	,#{teacher_add_day}
	,#{teacher_phone}
	,#{teacher_bank}
	,#{teacher_account}
	,#{teacher_paystep}
)
</insert>



</mapper>